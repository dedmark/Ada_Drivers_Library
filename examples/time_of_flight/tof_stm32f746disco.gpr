with "../../boards/stm32f746_discovery.gpr";

project TOF_STM32F746Disco extends "../common/common.gpr" is

  for Runtime ("Ada") use STM32F746_Discovery'Runtime ("Ada");
  for Target use "arm-eabi";
  for Main use ("main.adb");
  for Languages use ("Ada");
  for Source_Dirs use ("src");
  for Object_Dir use "obj";

  package Linker is
     for Default_Switches ("Ada") use ("-Wl,--print-memory-usage");
  end Linker;

end TOF_STM32F746Disco;
